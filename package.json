{
  "name": "gms-client",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "start:en": "ng serve --configuration=en",
    "start:pt": "ng serve --configuration=pt",
    "build": "ng build",
    "build:en": "ng build --configuration=en",
    "build:pt": "ng build --configuration=pt",
    "int:extract": "ng xi18n --output-path locale",
    "build-locale": "npm run build-locale:en && npm run build-locale:pt",
    "build-locale:en": "ng build --prod --i18n-locale en --i18n-format xlf --i18n-file src/locale/messages.en.xlf --output-path=dist/en --baseHref /en/",
    "build-locale:pt": "ng build --prod --i18n-locale pt --i18n-format xlf --i18n-file src/locale/messages.pt.xlf --output-path=dist/pt --baseHref /pt/"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~8.0.0",
    "@angular/common": "~8.0.0",
    "@angular/compiler": "~8.0.0",
    "@angular/core": "~8.0.0",
    "@angular/fire": "^5.2.1",
    "@angular/forms": "~8.0.0",
    "@angular/platform-browser": "~8.0.0",
    "@angular/platform-browser-dynamic": "~8.0.0",
    "@angular/router": "~8.0.0",
    "@ng-bootstrap/ng-bootstrap": "^4.2.1",
    "bootstrap": "^4.3.1",
    "chart.js": "^2.8.0",
    "firebase": "^6.1.1",
    "font-awesome": "^4.7.0",
    "jquery": "^3.4.1",
    "ng2-charts": "^2.3.0",
    "popper.js": "^1.15.0",
    "rxjs": "~6.5.2",
    "tslib": "^1.10.0",
    "zone.js": "~0.9.1"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.800.2",
    "@angular/cli": "~8.0.2",
    "@angular/compiler-cli": "~8.0.0",
    "@angular/language-service": "~8.0.0",
    "@types/node": "~12.0.8",
    "ts-node": "~8.2.0",
    "typescript": "~3.4.3"
  }
}
